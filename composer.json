{
    "name": "endeavors/maxmd-directutil",
    "description": "A MaxMD php api client to validate recipients",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Rodriguez",
            "email": "adam_rodriguez@bitbucket.org"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Endeavors\\MaxMD\\DirectUtil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\MaxMD\\DirectUtil\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bpedroza/maxmd-message.git"
        }
    ],
    "require" : {
        "endeavors/maxmd-apiauth": "0.1.*",
        "endeavors/maxmd-support": "~1.4.0",
        "endeavors/support-vo": "^2.0",
        "endeavors/maxmd-message": "version-updates-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
